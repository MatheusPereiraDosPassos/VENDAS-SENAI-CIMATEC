<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaProdutoLista</from-outcome>
            <to-view-id>/ProdutoLista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaVendedorLista</from-outcome>
            <to-view-id>/VendedorLista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaVenda</from-outcome>
            <to-view-id>/Venda.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ProdutoLista.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltarIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/ProdutoIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaVisualizar</from-outcome>
            <to-view-id>/ProdutoVisualizar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaeditar</from-outcome>
            <to-view-id>/ProdutoEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/ProdutoLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/VendedorLista.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltarIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/VendedorIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaeditar</from-outcome>
            <to-view-id>/VendedorEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaVisualizar</from-outcome>
            <to-view-id>/VendedorVisualizar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/VendedorLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>produtoFacade</managed-bean-name>
        <managed-bean-class>control.ProdutoFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/ProdutoIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltaParaListar</from-outcome>
            <to-view-id>/ProdutoLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ProdutoVisualizar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltaParaListar</from-outcome>
            <to-view-id>/ProdutoLista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaeditar</from-outcome>
            <to-view-id>/ProdutoEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ProdutoEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltaParaListar</from-outcome>
            <to-view-id>/ProdutoLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/VendedorIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltaParaListar</from-outcome>
            <to-view-id>/VendedorLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/VendedorEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltaParaListar</from-outcome>
            <to-view-id>/VendedorLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/VendedorVisualizar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltaParaListar</from-outcome>
            <to-view-id>/VendedorLista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaeditar</from-outcome>
            <to-view-id>/VendedorEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>vendaFacade</managed-bean-name>
        <managed-bean-class>control.VendaFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/Venda.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltaMenu</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
